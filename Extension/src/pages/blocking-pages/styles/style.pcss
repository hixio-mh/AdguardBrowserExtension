* {
    box-sizing: border-box;
}

html, body {
    width: 100%;
    height: 100%;
    font-size: 11px;
}

body {
    margin: 0;
    font-family: Arial, sans-serif;
}

.link {
    color: #4d9c5b;
    text-decoration: none;

    &:hover {
        text-decoration: underline;
    }

    &--alert {
        color: red;
    }

    &--btn {
        line-height: 42px;
        font-size: 15px;
        padding: 0 15px;
    }
}

.button {
    text-decoration: none;
    text-align: center;
    font-size: 15px;
    display: inline-block;
    outline: none;
    border: 0;
    background-color: transparent;
    cursor: pointer;
    border-radius: 3px;

    &:focus {
        box-shadow: 0 0 2px 1px rgba(74, 74, 74, .36);
    }

    &--green {
        background-color: #66b574;
        color: #fff;
        transition: 0.3s ease background-color;

        &:hover {
            background-color: #58a273;
        }
    }

    &--white {
        background-color: #fff;
        color: #232323;
        box-shadow: inset 0 0 0 1px #c4c4c4;
        transition: 0.3s ease background-color;

        &:hover {
            background-color: #f1f1f1;
        }
    }

    &--m {
        min-width: 180px;
        padding: 0 15px;
        line-height: 42px;
    }

    &--l {
        min-width: 100%;
        padding: 0 15px;
        line-height: 50px;
    }
}

.input {
    font-size: 1.4rem;
    text-align: center;
    outline: 0;
    border: 1px solid #e8e8e8;
    border-radius: 3px;

    &:focus {
        &::placeholder {
            opacity: 0;
        }
    }

    &.error {
        border-color: #DF3812;
    }

    &--l {
        width: 100%;
        padding: 11px;
    }
}

.help-text {
    color: #DF3812;
    margin-top: 0;
    margin-bottom: 20px;
    font-size: 14px;
}

.alert {
    width: 100%;
    height: 100%;
    position: relative;
    z-index: 5;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    padding: 20px 0;
    background-image: url("data:image/png;base64,");
    background-size: 15px;

    &--red {
        background-color: #94392D;
    }

    &--green {
        background-color: #537F5A;
    }

    &--gold {
        background-color: #b38237;
    }

    @media (max-width: 797px) {
        display: block;
        padding: 0;
    }

    &__in {
        width: 760px;
        background: #fff;
        box-shadow: 0 2px 15px rgba(0,0,0,0.3);
        overflow-y: auto;

        @media (max-width: 797px) {
            width: 100%;
            height: 100%;
            min-height: 100%;
        }
    }

    &__header {
        padding: 15px 70px;
        position: relative;

        &:before {
            content: "";
            width: 33px;
            height: 33px;
            display: block;
            position: absolute;
            left: 25px;
            top: calc(50% - 17px);
            background-repeat: no-repeat;
            background-size: 100%;
            background-image: url("../images/tick.svg");

            @media (max-width: 797px) {
                left: 15px;
                width: 26px;
                height: 26px;
                top: calc(50% - 13px);
            }
        }

        &--red {
            background-color: #ea2c13;
        }

        &--green {
            background-color: #66b574;
        }

        &--gold {
            background-color: #f5a623;
        }

        &-title {
            font-size: 18px;
            font-weight: 700;
            color: #fff;
            line-height: 30px;
            text-align: center;

            @media (max-width: 797px) {
                font-size: 14px;
                line-height: 24px;
            }
        }

        @media (max-width: 797px) {
            padding: 15px 45px;
        }
    }

    &__logo {
        display: block;
        width: 125px;
        height: 29px;
        margin: 0 auto 20px;
        background-image: url("../images/logo_adguard.svg");
    }

    &__pic {
        display: block;
        margin: 0 auto 20px;
        height: 130px;

        &--unsafe {
            width: 140px;
            position: relative;
            left: 20px;
        }

        &--parental {
            width: 105px;
        }

        &--rule {
            position: relative;
            width: 145px;
            right: 13px;
        }

        &--reputation {
            width: 202px;
            position: relative;
        }
    }

    &__body {
        padding: 20px 30px;

        &-title {
            text-align: center;
            line-height: 27px;
            font-size: 18px;
            max-width: 395px;
            margin: 0 auto 20px;

            @media (max-width: 797px) {
                max-width: 100%;
                font-size: 16px;
            }
        }

        &-desc {
            max-width: 575px;
            margin: 0 auto 20px;
            font-size: 14px;
            line-height: 22px;
            text-align: center;
        }
    }

    &__btns {
        display: flex;
        justify-content: center;

        @media (max-width: 797px) {
            width: 100%;
            display: block;
        }
    }

    &__btn {
        margin-right: 15px;

        &:last-child {
            margin-right: 0;
        }

        @media (max-width: 797px) {
            width: 100%;
            margin: 0 0 15px;

            &:last-child {
                margin: 0;
            }
        }
    }

    &__form {
        display: flex;
        justify-content: center;

        &-in {
            display: inline-flex;
            flex-direction: column;
            align-items: center;

            @media (max-width: 797px) {
                width: 100%;
            }
        }

        &-item {
            margin-bottom: 20px;

            &:last-child {
                margin-bottom: 0;
            }
        }
    }

    &__rule {
        font-family: 'Courier', arial;
        border: 1px solid #e8e8e8;
        font-size: 14px;
        font-weight: 700;
        padding: 12px;
        text-align: center;
        max-width: 374px;
        margin: 0 auto 28px;
        overflow-x: auto;
    }
}

.hero {
    margin: 0 auto;
    background-repeat: no-repeat;
    background-position: center;

    &--gray {
        background-image: url('../images/hero-gray.svg');
        width: 225px;
        height: 262px;
    }

    &--green {
        background-image: url('../images/hero-green.svg');
        width: 297px;
        height: 265px;
        background-position: -22px center;
    }

    &--red {
        background-image: url('../images/hero-red.svg');
        height: 265px;
        width: 284px;
        background-position: 30px center;
    }

    &--green-bear {
        width: 175px;
        height: 218px;
        background-image: url('../images/bear-green.svg');
    }
}

.reputation {
    background-color: #f5f5f5;
    position: relative;
    padding: 35px;

    &__ico {
        position: absolute;
        width: 64px;
        height: 35px;
        background-size: cover;
        top: 25px;
        right: 35px;
        background-image: url('data:image/jpeg;base64,');
    }

    &__label {
        margin-bottom: 35px;
        padding-right: 70px;
        font-size: 15px;
        font-weight: 700;
    }

    &__list {
        font-size: 14px;

        &-item {
            padding-left: 45px;
            margin-bottom: 15px;
            position: relative;

            &:before {
                content: "";
                position: absolute;
                left: 0;
                top: 3px;
                width: 11px;
                height: 11px;
                background-size: cover;
                background-image: url("../images/cross.svg");
            }

            &--disable {
                opacity: 0.5;
            }
        }
    }
}

[v-cloak] {
    display:none;
}

.hidden {
    display: none;
}